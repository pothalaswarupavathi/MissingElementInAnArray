import java.util.*;
class MissingEle {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int a[]=new int[n-1];
        for(int i=0;i<a.length;i++){
            a[i]=in.nextInt();
        }
        int sum=0;
        for(int i=0;i<a.length;i++){
            sum=sum+a[i];
        }
       int sum1=n*(n+1)/2;
        
       
        System.out.println(sum1-sum);
    }
}
